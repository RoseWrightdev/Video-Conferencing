# Common
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_ADDR=redis:6379

# Backend (Go)
PORT=8080
LOG_LEVEL=debug
REDIS_ENABLED=true
RUST_SFU_ADDR=rust-sfu:50051
RUST_SFU_HEALTH_CHECK_ENABLED=true
ALLOWED_ORIGINS=http://localhost:3000
GO_ENV=development
JWT_SECRET=development_secret_must_be_at_least_32_bytes_long_12345
SKIP_AUTH=true
DEVELOPMENT_MODE=true
AUTH0_DOMAIN=
AUTH0_AUDIENCE=
ENABLE_SFU=true
SFU_ADDR=rust-sfu:50051

# Rust SFU
RUST_LOG=debug
GRPC_PORT=50051
HTTP_METRICS_PORT=3030

# Python Services
PYTHONUNBUFFERED=1

# Frontend
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=
AUTH0_ISSUER=
# AUTH0_AUDIENCE is shared with backend
NEXTAUTH_SECRET=
NEXTAUTH_URL=
NEXT_PUBLIC_WS_URL=

# Grafana
GF_SECURITY_ADMIN_PASSWORD=admin
